
    <% @users.each do |u|%>
    
        <%= image_tag u.image_link, class:'rounded-circle', alt:'Profile picture' %>
        <%= link_to u.email, user_path(u)%>
        <small><%=u.email%></small>
        <small class='text'>Joined <%= time_ago_in_words u.created_at %> ago</small>
     
   <% end %>

        

  